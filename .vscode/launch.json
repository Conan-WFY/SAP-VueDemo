{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run conanvue-conanvue",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/conanvue/index.html"
      ],
      "cwd": "${workspaceFolder}/conanvue",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "dist",
        "run.config": "{\"handlerId\":\"html5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/conanvue/conanvue\"}"
      },
      "envFile": "${workspaceFolder}/conanvue/.env1",
      "preLaunchTask": "Run conanvue"
    }
  ]
}